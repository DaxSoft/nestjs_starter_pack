{
  "name": "@travelerspen/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "proprietary",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker compose up -d -p backend_campy",
    "database": "prisma studio"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.7.0",
    "@aws-sdk/client-s3": "^3.317.0",
    "@aws-sdk/client-ses": "^3.316.0",
    "@aws-sdk/s3-request-presigner": "^3.317.0",
    "@nestjs-addons/in-memory-db": "^3.0.3",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/cli": "^9.4.1",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/testing": "9.4.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "^9.4.0",
    "@prisma/client": "^4.13.0",
    "@t3-oss/env-core": "^0.2.2",
    "@upstash/ratelimit": "^0.4.2",
    "@upstash/redis": "^1.20.4",
    "@vorlefan/path": "^1.8.35",
    "ably": "^1.2.39",
    "apollo-server-express": "3.11.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "file-type": "^18.2.1",
    "graphql": "^16.6.0",
    "graphql-tools": "8.3.20",
    "graphql-upload": "12.0.0",
    "helmet": "^6.1.5",
    "isomorphic-fetch": "^3.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.13.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.0",
    "run-script-webpack-plugin": "^0.1.1",
    "rxjs": "7.8.0",
    "sharp": "^0.32.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-node-externals": "^3.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "tsconfig-paths": "4.2.0"
  }
}
